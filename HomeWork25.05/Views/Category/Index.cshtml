@{
    List<Category> categories = ViewBag.Categories;
    <table class="table-bordered table-hover table-striped table">
        <thead>
            <tr>
                <td>Id</td>
                <td>Category name</td>
            </tr>
        </thead>
        <tbody>
            @foreach(var category in categories)
            {
                <tr>
                    <td>@category.Id</td>
                    <td>@category.CategoryName</td>
                </tr>
            }
        </tbody>
    </table>
}